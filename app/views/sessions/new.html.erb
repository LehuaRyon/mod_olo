<h1>Log In Here:</h1>

<%= flash[:error] %>

<%= form_with url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit 'Log In' %>
<% end %>

<h2>Or Log In with Google:</h2>

<%= button_to 'Google', '/auth/google_oauth2' %>